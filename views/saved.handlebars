    <div id="index-banner" class="parallax-container">
    <div class="section no-pad-bot">
      <div class="container">
        <br><br>
        <h1 class="header center orange-text text-lighten-2">MongoDB News Scraper</h1>
        <div class="row center">
          <h4 class="header col s12 light">Saved Articles</h4>
        </div>


          <!-- Modal Structure -->
          <div id="notes" class="modal modal-fixed-footer">
            <div class="modal-content" id="note">
                <h5 id="note-title"></h5>
                <label for='titleinput'>Text</label>
                <input id='titleinput' name='title' placeholder='Note Text'>
                <button data-id='' id='savenote'>Save Note</button>
            </div>

            <div class="modal-footer">
              <a href="#!" class="modal-action modal-close waves-effect waves-green btn-flat ">x</a>
            </div>
          </div>
        <br><br>

      </div>
    </div>
    <div class="parallax"><img src="/assets/images/deli.jpg" alt="Unsplashed background img 1"></div>
  </div>

  <div class="container">
    <div id="wrapper">
      {{#each article}}
              <div class="col s12 m7">
              <h5 class="header">{{this.title}}</h5>
              <div class="card horizontal">
                <div class="card-image">
                  <img src="{{this.image}}">
                </div>
                <div class="card-stacked">
                  <div class="card-content">
                    <a href="{{this.link}}">{{this.link}}</a>
                  </div>
                  <div class="card-action">
                    <a id="add-note" data-id="{{this._id}}">Add Note</a>
                    <a id="delete-note" class="red-text">Delete Note</a>
                  </div>
                </div>
              </div>
            </div>
      {{/each}}
      {{!-- <div id="articles"></div>
      <div id="notes"></div> --}}
    </div>
  </div>


  <div class="parallax-container valign-wrapper">
    <div class="section no-pad-bot">
      <div class="container">
        <div class="row center">
        </div>
      </div>
    </div>
    <div class="parallax"><img src="/assets/images/beef.jpg" alt="Unsplashed background img 3"></div>
  </div>

